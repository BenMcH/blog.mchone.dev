{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:posts && npm run build:css && remix build",
    "build:posts": "npx ts-node scripts/buildBlogCache.ts",
    "build:css": "tailwindcss -i ./app/styles/tailwind-starter.css -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:posts\" \"npm run dev:css\" \"cross-env NODE_ENV=development remix dev\"",
    "dev:css": "tailwindcss -i ./app/styles/tailwind-starter.css -o ./app/tailwind.css --watch",
    "dev:posts": "npx ts-node scripts/buildBlogCache.ts watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.0.2",
    "@octokit/plugin-throttling": "^3.6.2",
    "@octokit/rest": "^18.12.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.5.0",
    "mdx-bundler": "^9.0.0",
    "p-queue": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reading-time": "^1.5.0",
    "rehype-highlight": "^5.0.2",
    "remark-toc": "^8.0.1",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.0.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
